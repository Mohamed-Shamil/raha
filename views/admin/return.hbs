          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Return Order Notifications</h5>

              <!-- Accordion without outline borders -->
              {{#each returns}}
              <div class="accordion accordion-flush" id="accordionFlushExample">
                <div class="accordion-item">
                  
                  <h1 class="accordion-header" id="flush-headingOne">
                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapseOne" aria-expanded="false" aria-controls="flush-collapseOne">
                      OrderId:{{_id}}
                    </button>
                  </h1>
                  <div id="flush-collapseOne" class="accordion-collapse collapse" aria-labelledby="flush-headingOne" data-bs-parent="#accordionFlushExample">
                   <div class="row">

                      <div class="col-lg-4 col-md-3">
                         <img src="/public/pictures/product-images/{{product.img.[0]}}" alt="">
                         <b>hii</b>
                        

                  </div>
                  <div class="col-lg-4 col-md-3">
                     <div class="accordion-body"> {{deliveryDetails.firstName}} {{deliveryDetails.lastName}}
                      <p>Payment Method: <b>{{paymentMethod}}</b> </p>
                      <p>Amount to be Refunded : </p>
                      <b>{{product.price}}</b>
                     </div>
                  </div>
                  <div class="col-lg-4 col-md-3">

                  </div>

                   </div>
                
                  
                  

                  </div>
                </div>
              </div><!-- End Accordion without outline borders -->
              {{/each}}

              {{!--  --}}
              {{#each returns}}
